{
  "kind": "collectionType",
  "collectionName": "members",
  "info": {
    "name": "Members",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "unique": true
    },
    "alumni": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "passoutYr": {
      "type": "integer"
    },
    "position": {
      "type": "string",
      "default": "Executive Member",
      "required": true
    },
    "roll": {
      "type": "uid",
      "required": true,
      "private": true
    },
    "contacts": {
      "type": "component",
      "repeatable": false,
      "component": "members.social"
    },
    "sigs": {
      "collection": "sig"
    },
    "image": {
      "type": "string",
      "regex": "https?:\\/\\/(www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-a-zA-Z0-9()@:%_\\+.~#?&//=]*)",
      "unique": true
    }
  }
}
