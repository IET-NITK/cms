{
  "kind": "collectionType",
  "collectionName": "members",
  "info": {
    "name": "Members",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "unique": true
    },
    "alumni": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "passoutYr": {
      "type": "integer"
    },
    "position": {
      "type": "string",
      "default": "Executive Member",
      "required": true
    },
    "roll": {
      "type": "uid",
      "required": true,
      "private": true
    },
    "contacts": {
      "type": "component",
      "repeatable": false,
      "component": "members.social"
    },
    "sigs": {
      "collection": "sig"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "Internship": {
      "type": "string",
      "private": true
    },
    "Placements": {
      "type": "string",
      "private": true
    }
  }
}
