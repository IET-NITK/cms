{
  "kind": "collectionType",
  "collectionName": "smps",
  "info": {
    "name": "SMP",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "url": {
      "type": "string",
      "regex": "[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-a-zA-Z0-9()@:%_\\+.~#?&//=]*)",
      "required": true
    },
    "SIG": {
      "model": "sig"
    },
    "members": {
      "collection": "members"
    },
    "year": {
      "type": "integer",
      "required": true,
      "default": 2021
    }
  }
}
